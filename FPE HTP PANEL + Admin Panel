local coreReplication = replicatedStorage:WaitForChild("Events"):WaitForChild("Core_Replication")

local t = 0
local runService = game:GetService("RunService")

runService.RenderStepped:Connect(function(dt)
    t = t + dt
    local r = math.sin(t * 2) * 0.5 + 0.5
    local g = math.sin(t * 2 + 2) * 0.5 + 0.5
    local b = math.sin(t * 2 + 4) * 0.5 + 0.5

    local rainbowColor = Color3.new(r, g, b)

    local args = {
        [1] = "Text",
        [2] = "Change_Color",
        [3] = textLabel,
        [4] = rainbowColor,
        [5] = "Role_Color"
    }

    coreReplication:FireServer(unpack(args))
end)

    Fluent:Notify({
        Title = "Yessir! üòé",
        Content = "Executed Rainbow Tag!",
        SubContent = "It should pop above your head ina current moment‚Ä¶", -- Optional
        Duration = 5 -- Set to nil to make the notification not disappear
    })
       end
